{
	"name" : "edged/mulungu",
	"description" : "PHP Development Framework",
	"authors" : [{
			"name" : "Kwasi Gyasi-Agyei",
			"email" : "kwasi.gyasiagyei@edged.co.za"
		}
	],
	"autoload" : {
		"psr-0" : {
			"Edged\\" : "src/"
		}
	},
	"require" : {
		"php" : ">=5.3.3",
		"guzzlehttp/guzzle" : "6.0.2",
		"anahkiasen/underscore-php" : "1.3.0",
		"nesbot/carbon" : "1.13.0",
		"respect/validation" : "0.6.1",
		"tecnickcom/tcpdf": "~6.2",
        "phpoffice/phpexcel": "1.8.*",
        "pelago/emogrifier": "@dev",
	    "danielstjules/stringy": "~2.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.1.*"
	},
	"config" : {
		"bin-dir" : "bin",
		"secure-http": false
	},
	"keywords" : [
		"Mulungu",
		"Symfony bundle",
		"Mulungu bundle",
		"Edged"
	],
	"prefer-stable" : true
}